{
  "expo": {
    "name": "DeRemate Logistica",
    "slug": "deremate_front_reactnative_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/derematelogistica.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.deremate_front_reactnative_app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.deremate_front_reactnative_app",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/deremateNotification.png"
    },
    "plugins": [
      "expo-secure-store",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging"
    ],
    "notification": {
      "icon": "./assets/deremateNotification.png",
      "color": "#3a86ff",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Nuevo Paquete Disponible"
    },
    "extra": {
      "eas": {
        "projectId": "18493a92-42b2-4c73-802e-14ec5db62640"
      }
    }
  }
}
